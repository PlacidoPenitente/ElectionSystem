<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:ElectionSystem.Resources"
                    xmlns:viewmodels="clr-namespace:ElectionSystem.ViewModels"
                    xmlns:views="clr-namespace:ElectionSystem.Views">
    <!--Login-->
    <DataTemplate DataType="{x:Type viewmodels:LoginViewModel}">
        <views:LoginView/>
    </DataTemplate>
    
    <!--Workspace-->
    <DataTemplate DataType="{x:Type viewmodels:WorkspaceViewModel}">
        <views:WorkspaceView/>
    </DataTemplate>
    
    <!--ReadVotersView-->
    <DataTemplate DataType="{x:Type viewmodels:ReadVotersViewModel}">
        <views:ReadVotersView/>
    </DataTemplate>
</ResourceDictionary>