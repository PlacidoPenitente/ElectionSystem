<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:viewmodels="clr-namespace:ElectionSystem.ViewModels"
                    xmlns:views="clr-namespace:ElectionSystem.Views">
    <!--Login-->
    <DataTemplate DataType="{x:Type viewmodels:LoginViewModel}">
        <views:LoginView/>
    </DataTemplate>
    
    <!--Workspace-->
    <DataTemplate DataType="{x:Type viewmodels:WorkspaceViewModel}">
        <views:WorkspaceView/>
    </DataTemplate>
    
    <!--ReadVotersView-->
    <DataTemplate DataType="{x:Type viewmodels:ReadVotersViewModel}">
        <views:ReadVotersView/>
    </DataTemplate>
    
    <!--RegisterVoters-->
    <DataTemplate DataType="{x:Type viewmodels:CreateUpdateVoterViewModel}">
        <views:CreateUpdateVoterView/>
    </DataTemplate>

    <!--ReadUsersView-->
    <DataTemplate DataType="{x:Type viewmodels:ReadUsersViewModel}">
        <views:ReadUsersView/>
    </DataTemplate>

    <!--CreateUser-->
    <DataTemplate DataType="{x:Type viewmodels:CreateUpdateUserViewModel}">
        <views:CreateUpdateUserView/>
    </DataTemplate>

    <!--Dialog-->
    <DataTemplate DataType="{x:Type viewmodels:DialogViewModel}">
        <views:DialogView/>
    </DataTemplate>
</ResourceDictionary>